<%- include('../header') %>
<div class="ui container">
    <div class="ui comments">

            <div class="comment">
                <a href="/message/<%= message.id %>" class="avatar">
                    <i class="icon comment outline"></i>
                </a>
                <div class="content">
                    <div class="metadata">
                        <div class="date">
                            <%= message.create_at.fromNow() %>
                        </div>
                    </div>
                    <div class="test"><%= message.content %></div>
                </div>
            </div>       

    </div>
</div>
<%- include('../footer') %>